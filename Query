CREATE TABLE HEARING_TB(
RECORDID VARCHAR2(12 BYTE) PRIMARY KEY,
CASENUMBER VARCHAR2(30 BYTE) NOT NULL,
PETITIONER VARCHAR2(50 BYTE) NOT NULL,
RESPONDENT VARCHAR2(50 BYTE) NOT NULL,
HEARING_DATE DATE NOT NULL,
COURTROOM VARCHAR2(10 BYTE),
REMARKS VARCHAR2(150 BYTE)
);
CREATE SEQUENCE HEARING_SEQ
START WITH 10
INCREMENT BY 1
MAXVALUE 99;
